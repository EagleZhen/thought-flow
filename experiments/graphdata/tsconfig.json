{
	"compilerOptions": {
		"module": "ESNext",
		"target": "ES2022",
		"lib": [
			"ES2022"
		],
		"sourceMap": true,
		"rootDir": "src",
		"strict": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"moduleResolution": "Node",
